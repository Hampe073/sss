LoginButton.Name = "LoginButton"
LoginButton.Parent = ScreenGui
LoginButton.BackgroundColor3 = Color3.new(0.0588235, 0.0588235, 0.0588235)
LoginButton.BorderColor3 = Color3.new(0.137255, 0.137255, 0.137255)
LoginButton.Position = UDim2.new(0, 723, 0, 426)
LoginButton.Size = UDim2.new(0, 103, 0, 25)
LoginButton.Font = Enum.Font.SourceSans
LoginButton.Text = "login"
LoginButton.TextColor3 = Color3.new(1, 1, 1)
LoginButton.TextSize = 14
LoginButton.MouseButton1Down:connect(function()
local user = false
local i = 1
local j = 100
while ( i < j ) do
    if (UsernameTextBox.Text == username[i] and PasswordTextBox.Text == keys[i]) then 
        i = 999999
        user = true
        wait(0.5)
        LoginExitButton.Visible = false
        LockIcon.Visible = false
        PersonIcon.Visible = false
        LoginButton.Visible = false
        PasswordTextBox.Visible = false
        UsernameTextBox.Visible = false
        LoginButton.Visible = false
        Logo.Visible = false
        Name.Visible = false
        LoginSecondBG.Visible = false
        LoginMainBG.Visible = false
    AnimationMainBG.Visible = true
    AnimationSpinningLogo.Visible = true
    wait(3)
        AnimationMainBG.Visible = false
        AnimationSpinningLogo.Visible = false
        InjectMainBG.Visible = true
        InjectSecondBG.Visible = true
        InjectBGG2.Visible = true
        InjectChooseBG.Visible = true
        InjectTranBG2.Visible = true
        InjectLine.Visible = true
        InjectTranBG1.Visible = true
        InjectLogo.Visible = true
        InjectName.Visible = true
        InjectChooseCB.Visible = true
        InjectChooseName.Visible = true
        InjectSubTime.Visible = true
        InjectLoadButton.Visible = true
        InjectStatus.Visible = true
        InjectUpdated.Visible = true
        InjectCheatName2.Visible = true
        InjectWelcome.Visible = true
        InjectExit.Visible = true
    end
    i = i + 1
end
if user == false then
    game.Players.LocalPlayer:Kick("\n[brimordial]\nUnauthorized Access (If you belive this is false contact support.)")
end
end)

